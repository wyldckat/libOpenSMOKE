FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0.75);

boundaryField
{
    inletfuel           
    {
	type 		fixedValue;
        value           uniform (0 0 76);
    }
    inletair           
    {
	type 		fixedValue;
        value           uniform (0 0 0.75);
    }

    outlet          
    {
	type 		zeroGradient;
    }

    leftside       
    {
	type 		zeroGradient;
    }

    burnerwall      
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    front
    {
        type            wedge;
    }

    back
    {
        type            wedge;
    }

    axis
    {
        type            empty;
    }
}


