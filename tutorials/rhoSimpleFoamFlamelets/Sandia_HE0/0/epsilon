FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inletfuel
    {
        type            fixedValue;
        value           uniform 3.86e6;
    }
    
    inletair
    {
        type            fixedValue;
        value           uniform 1;
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    axis
    {
        type            empty;
    }
    
    leftside
    {
        type 		zeroGradient;
    }
    
    burnerwall
    {
        type            compressible::epsilonWallFunction;
        value           uniform 0;
    }
    
    front
    {
        type            wedge;
    }
    
    back
    {
        type            wedge;
    }
}


